{
  "name": "telepath",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npm run test:api && npm run test:game",
    "test:api": "npm run test:api:compile && npm run test:api:run",
    "test:api:compile": "tsc -p tsconfig.api-tests.json",
    "test:api:run": "node --test --test-concurrency=1 .test-dist/api/__tests__/*.test.js",
    "test:game": "npm run test:game:compile && npm run test:game:run",
    "test:game:compile": "tsc -p tsconfig.game-tests.json",
    "test:game:run": "node --test --test-concurrency=1 .test-dist-game/lib/__tests__/*.test.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "framer-motion": "^12.34.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^25.3.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.5"
  }
}
